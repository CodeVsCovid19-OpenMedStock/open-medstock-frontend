{
  "name": "open-medstock",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npm-run-all --parallel serve:frontend serve:mock-backend",
    "serve:frontend": "vue-cli-service serve",
    "serve:mock-backend": "json-server --watch db.json --routes routes.json",
    "serve:prod": "npm run build && serve -s dist -l 4040",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@mdi/font": "^3.6.95",
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "roboto-fontface": "*",
    "vue": "^2.6.11",
    "vue-router": "^3.1.5",
    "vuelidate": "^0.7.5",
    "vuetify": "^2.2.11"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-plugin-router": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.1.2",
    "json-server": "^0.16.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "serve": "^11.3.0",
    "vue-cli-plugin-vuetify": "~2.0.5",
    "vue-template-compiler": "^2.6.11"
  }
}
